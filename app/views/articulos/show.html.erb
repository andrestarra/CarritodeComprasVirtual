<h1><%= @articulo.nombre %></h1>

<table>
  <tbody>
    <tr>
      <th scope='row'>Precio:</th>
      <td><%= @articulo.precio %></td>
    </tr>
    <tr>
      <th scope='row'>Cantidad:</th>
      <td><%= @articulo.cantidad %></td>
    </tr>
  </tbody>
</table>

<%= link_to 'Editar', edit_articulo_path(@articulo) %>
<%= link_to 'Regresar', articulos_path %>